# Datadog MCP Server - Environment Variables
# Copy this file to .env for local development

# Keycard Configuration (from your Keycard Application Credentials)
KEYCARD_ZONE_ID=your_zone_id
KEYCARD_CLIENT_ID=your_client_id
KEYCARD_CLIENT_SECRET=your_client_secret

# Server Configuration
MCP_SERVER_URL=http://localhost:8000
PORT=8000

# Datadog API Credentials
DD_API_KEY=your_datadog_api_key
DD_APPLICATION_KEY=your_datadog_application_key
